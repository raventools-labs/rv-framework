<?xml version="1.0" encoding="UTF-8"?>
<ehcache:config
	xmlns:ehcache="http://www.ehcache.org/v3"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb">

	<ehcache:cache-template name="LocaleTemplate">
	
		<ehcache:key-type>java.lang.String</ehcache:key-type>
		<ehcache:value-type>java.lang.String</ehcache:value-type>
		<ehcache:heap unit="entries">1000</ehcache:heap>
		
	</ehcache:cache-template>

	<ehcache:cache alias="Locale.es_ES" uses-template="LocaleTemplate"></ehcache:cache>

	<ehcache:cache alias="Locale.en_EN" uses-template="LocaleTemplate"></ehcache:cache>
	
</ehcache:config>